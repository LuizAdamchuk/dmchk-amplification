datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt DateTime  @default(now())
  id        String    @id @default(cuid())
  mfes      Mfe?
  name      String?
  updatedAt DateTime  @updatedAt
  username  String?   @unique
  usersMfes UserMfe[]
}

model Mfe {
  active                 Boolean?
  authenticationRequired Boolean?
  createdAt              DateTime  @default(now())
  description            String?
  id                     String    @id @default(cuid())
  key                    String?   @unique
  mfe                    Mfe?      @relation(name: "MfeOnMfes", fields: [mfeId], references: [id])
  mfeId                  String?
  mfes                   Mfe[]     @relation(name: "MfeOnMfes")
  updatedAt              DateTime  @updatedAt
  user                   User?     @relation(fields: [userId], references: [id])
  userId                 String?   @unique
  usersMfes              UserMfe[]
}

model UserMfe {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  mfe       Mfe[]
  mfeId     String?
  updatedAt DateTime @updatedAt
  user      User[]
  userId    String?
}

